{
  "name": "trello-nodejs-typescript",
  "version": "1.0.0",
  "author": "Khang Nguyen",
  "license": "ISC",
  "description": "A Trello clone backend with Node.js and TypeScript",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc && tsc-alias",
    "start": "node ./dist/server.js",
    "dev": "tsx watch ./src/server.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "prepare": "husky",
    "cm": "git-cz"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mongodb": "^6.20.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/cz-commitlint": "^20.1.0",
    "@eslint/js": "^9.38.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.9.1",
    "commitizen": "^4.3.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "inquirer": "^9.3.8",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "homepage": "https://github.com/ngkhang/e-commerce-nodejs-typescript#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngkhang/e-commerce-nodejs-typescript.git"
  },
  "keywords": [
    "nodejs",
    "typescript"
  ]
}
